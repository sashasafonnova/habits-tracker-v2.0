{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "ESNext",
    "target": "es5",
    "jsx": "react-jsx",
    "allowJs": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "*": ["./src/*"],
    }
    },
    "ts-node": {
      "compilerOptions": {
        "module": "CommonJS"
      }
    },
    "include": [
      "./config/jest/setupTests.ts",
      "./src/**/*.ts",
      "./src/**/*.tsx", "src/entities/TrackProfile/model/selectors/selectors/detailsErrorSelector.ts",
    ]
  }
